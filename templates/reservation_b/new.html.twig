{% extends 'frontOffice.html.twig' %}



{% block body %}
    <div class="item-details-page">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-heading">
                        <div class="line-dec"></div>
                        <h2>Apply your team For <em>The Game</em> Here.</h2>
                    </div>
                </div>
                <div class="col-lg-12">
                    {{ form_start(form, {'attr': {'id': 'contact'}}) }}
                    <div class="row">
                        <div class="col-lg-4">
                            <fieldset>
                                {{ form_label(form.oldrank) }}
                                {{ form_widget(form.oldrank, {'attr': { 'autocomplete': 'on'}}) }}
                            </fieldset>
                        </div>
                        <div class="col-lg-4">
                            <fieldset>
                                {{ form_label(form.newrank) }}
                                {{ form_widget(form.newrank, {'attr': { 'autocomplete': 'on'}}) }}
                            </fieldset>
                        </div>
                        <div class="col-lg-4">
                            <fieldset>
                                {{ form_label(form.newprice) }}
                                {{ form_widget(form.newprice, {'attr': { 'autocomplete': 'on'}}) }}
                            </fieldset>
                        </div>
                        <div class="col-lg-8">
                            <fieldset>
                                <button  type="submit"  class="orange-button">Book ur booster</button>
                            </fieldset>
                        </div>
                        </div>
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
    </div>
{% endblock %}