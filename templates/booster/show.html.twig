<!DOCTYPE html>

<title>Booster</title>

{% block body %}
    <h1>Booster</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ booster.id }}</td>
            </tr>
            <tr>
                <th>Rang</th>
                <td>{{ booster.rang }}</td>
            </tr>
            <tr>
                <th>Lane</th>
                <td>{{ booster.lane }}</td>
            </tr>
            <tr>
                <th>Op_gg</th>
                <td>{{ booster.opGg }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ booster.description }}</td>
            </tr>
            <tr>
                <th>Language</th>
                <td>{{ booster.language }}</td>
            </tr>
            <tr>
                <th>Period</th>
                <td>{{ booster.period }}</td>
            </tr>
            <tr>
                <th>Wallet</th>
                <td>{{ booster.wallet }}</td>
            </tr>
            <tr>
                <th>Price</th>
                <td>{{ booster.price }}</td>
            </tr>
            <tr>
                <th>Availability</th>
                <td>{{ booster.availability ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_booster_index') }}">back to list</a>

    <a href="{{ path('app_booster_edit', {'id': booster.id}) }}">edit</a>

    {{ include('booster/_delete_form.html.twig') }}
