<div class="item-details-page">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="section-heading">
          <div class="line-dec"></div>
          <h2>Apply For <em>Booster</em> Reservation Here.</h2>
        </div>
      </div>
      <div class="col-lg-12">
        {{ form_start(form, {'attr': {'id': 'contact'}}) }}
        <div class="row">
          <div class="col-lg-4">
            <fieldset>
              {{ form_label(form.Oldrank) }}
              {{ form_widget(form.Oldrank, {'attr': {'placeholder': 'Ex. Bronze'}}) }}
            </fieldset>
          </div>
          <div class="col-lg-4">
            <fieldset>
              {{ form_label(form.Newrank) }}
              {{ form_widget(form.Newrank, {'attr': {'placeholder': 'Ex. Diamond'}}) }}
            </fieldset>
          </div>
          <div class="col-lg-4">
            <fieldset>
              {{ form_label(form.Prix) }}
              {{ form_widget(form.Prix) }}
            </fieldset>
          </div>
          
          <div class="col-lg-8">
            <fieldset>
                {{ form_widget(form.Save, {'attr': {'class': 'orange-button', 'label':'Save'}}) }}
            </fieldset>
          </div>
        </div>
        {{ form_end(form) }}
        
      </div>
    </div>
  </div>
</div>